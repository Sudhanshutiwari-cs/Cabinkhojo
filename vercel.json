{
  "headers": [
    {
      "source": "/fonts/inter-var-latin.woff2",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],

  "rewrites": [
    {
      "source": "/forgot-password/:path*",
      "destination": "/404"
    },
    {
      "source": "/guard/:path*",
      "destination": "/404"
    },
    {
      "source": "/hod/:path*",
      "destination": "/404"
    },
    {
      "source": "/student/:path*",
      "destination": "/404"
    },
    {
      "source": "/reset-password/:path*",
      "destination": "/404"
    },
    {
      "source": "/service/:path*",
      "destination": "/404"
    }
  ]
}
